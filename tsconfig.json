{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "preserveSymlinks": true,
    "esModuleInterop": true,
    "declaration": true,
    "strict": true,
  },
  "exclude": [
    "node_modules"
  ]
}